{
  "name": "wljs-interpreter",
  "version": "0.6.0",
  "description": "A core library and WL interpreter written in pure JS",
  "defaultPackage": true,
  "wljs-meta": {
    "js": [
      "dist/interpreter.js",
      "src/core.js",
      "src/settings.js"
    ],
    "minjs": [
      "dist/interpreter.min.js",
      "src/core.js",
      "src/settings.js"
    ],
    "frontend": "src/Definitions.wl",
    "kernel": "src/Definitions.wl",
    "autocomplete": "src/autocomplete.js",
    "important": true,
    "category": "Internal",
    "figureExportRequired": true,
    "cellExportRequired": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JerryI/wljs-interpreter"
  },
  "author": "Kirill Vasin",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/JerryI/wljs-interpreter/issues"
  },
  "homepage": "https://github.com/JerryI/wljs-interpreter#readme",
  "scripts": {
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/rollup --config rollup.config.mjs",
    "restore": "mkdir _temp && cd _temp && git clone --branch base https://github.com/JerryI/wljs-interpreter && mv wljs-interpreter/.git ../.git && cd .. && rm -rf _temp"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.4",
    "fflate": "^0.8.2",
    "rollup": "^3.21.6"
  },
  "staticFiles": [
    {
      "staticPath": "static"
    },
    {
      "staticPath": "src"
    }
  ]
}

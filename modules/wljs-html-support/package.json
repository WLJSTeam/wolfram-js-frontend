{
  "name": "wljs-html-support",
  "version": "0.0.4",
  "description": "HTML/WSP cells support for WLJS Editor",
  "scripts": {
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/rollup --config rollup.config.mjs",
    "restore": "mkdir _temp && cd _temp && git clone --branch base https://github.com/JerryI/wljs-html-support && mv wljs-html-support/.git ../.git && cd .. && rm -rf _temp"
  },
  "defaultPackage": true,
  "wljs-meta": {
    "frontend" : "src/Frontend.wl",
    "js": "src/kernel.js",
    "minjs": "src/kernel.js",
    "styles": "src/style.css",
    "category": "Cell types",
    "cellExportRequired": true,
    "deferred": ["packages/WSP.wl"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/JerryI/wljs-html-support"
  },
  "dependencies": {
    "rollup": "^3.21.6"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  }
}

window.globalPoll.handlers.push(() => {\n        const form = \
document.getElementById('<FormId/>');\n        if (!form) return;\n      \n   \
     const selectedLanguage = form.<NameValue/>.value;\n        if \
(!selectedLanguage) return;\n          \
window.globalPoll.results['<NameValue/>'] = selectedLanguage;\n      });\n    \
</script>\n  </div>\n]\n\nOptions[PollForm] = {\"Title\" -> \"Generic\", \
